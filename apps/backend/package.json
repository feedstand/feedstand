{
  "name": "@feedstand/backend",
  "type": "module",
  "scripts": {
    "dev": "tsx --env-file=.env --watch index.ts",
    "play": "tsx --env-file=.env --watch play.ts",
    "test": "vitest run",
    "start": "tsx index.ts",
    "db:seed": "tsx database/tasks/seed.ts",
    "db:truncate": "tsx database/tasks/truncate.ts",
    "db:ready": "tsx database/tasks/ready.ts",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "lint": "npm run lint:tsc",
    "lint:tsc": "tsc --pretty --noEmit",
    "lint:prettier:base": "prettier --write",
    "lint:prettier": "npm run lint:prettier:base -- ."
  },
  "dependencies": {
    "@bull-board/hono": "^6.0.0",
    "@date-fns/tz": "^1.0.0",
    "@hono/node-server": "^1.0.0",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.0",
    "@sentry/node": "^8.0.0",
    "@sentry/profiling-node": "^8.0.0",
    "axios": "^1.0.0",
    "bullmq": "^5.0.0",
    "cheerio": "^1.0.0",
    "date-fns": "^4.0.0",
    "drizzle-kit": "^0.30.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.6.0",
    "feedsmith": "^1.0.0",
    "hono": "^4.0.0",
    "ioredis": "^5.0.0",
    "lodash-es": "^4.0.0",
    "normalize-url": "^8.0.0",
    "postgres": "^3.0.0",
    "tsx": "^4.0.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@types/lodash-es": "^4.0.0",
    "@types/node": "^22.0.0",
    "msw": "^2.0.0",
    "vitest": "^3.0.0"
  }
}
