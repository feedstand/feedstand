    "@hono/zod-openapi": "^1.1.4",
    "@node-rs/xxhash": "^1.7.6",
    "@sentry/node": "^10.23.0",
    "bullmq": "^5.63.0",
    "cacheable-lookup": "^7.0.0",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.8.3",
    "entities": "^7.0.0",
    "feedscout": "^1.0.0-beta.1",
