{
    "name": "feedstand",
    "type": "module",
    "devDependencies": {
        "husky": "^9.1",
        "typescript": "^5.5"
    },
    "engines": {
        "node": ">=22.8",
        "npm": ">=10.8"
    },
    "scripts": {
        "prepare": "husky",
        "dev": "docker compose --env-file=apps/backend/.env -f .docker/docker-compose.dev.yml up --build --force-recreate --renew-anon-volumes",
        "test": "docker compose --env-file=apps/backend/.env -f .docker/docker-compose.test.yml up --build --force-recreate --renew-anon-volumes --abort-on-container-exit"
    },
    "workspaces": [
        "apps/*"
    ]
}
